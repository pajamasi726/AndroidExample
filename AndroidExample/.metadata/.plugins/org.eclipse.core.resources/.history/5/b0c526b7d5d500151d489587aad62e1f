package com.example.exam12_service;

public class CustomThread extends Thread {

	
	@Override
	public void run() {
		int i = 0;
		while (true) {
			i++;
			try {
				Thread.sleep(1000);
			} 
			catch (InterruptedException e) {
				this.interrupt();
				System.out.println("interrupt");
			}
		}
	}
}
