public class CustomAdapter extends FragmentPagerAdapter{

	int PAGE = 3;
	
	public CustomAdapter(FragmentManager fm) {
		super(fm);
		// TODO Auto-generated constructor stub
	}

	@Override
	public Fragment getItem(int position) {
		Fragment frag;
		
		
		return null;
	}

	@Override
	public int getCount() {
		// TODO Auto-generated method stub
		return PAGE;
	}

}
