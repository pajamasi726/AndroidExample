package com.example.exam12_service;

public class CustomThread extends Thread {

	@Override
	public void run() {
		int i = 0;

		while (this.interrupted()) {
			i++;
			try {
				Thread.sleep(1000);
			} 
			catch (Exception e) {
			}

			System.out.println("Thread sleep : " + i);
		}

		System.out.println("Thread end");
		super.run();
	}
}
